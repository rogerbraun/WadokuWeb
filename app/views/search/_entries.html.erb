
<div id="entries" class="entries">
  <% entries.each do |entry| %>
    <div class="entry <%= cycle "even", "odd" %>">
      <p class="hanging-indent" ><%=entry.full_html(root_url) %> <br /><%= link_to t("entry.detail"), entry_path(entry) %></p>
    </div>
  <% end %>
</div>
<div class="actions-bar wat-cf">
  <div class="actions">
  <%= will_paginate entries  %>
  </div>
</div>
